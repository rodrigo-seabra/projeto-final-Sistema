@model LookingThings.Models.Observacoes

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes da observação n°: @Html.DisplayTextFor(model => model.ObservacoesId)</h1>

<div>
    <h4>Observações</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ObservacoesDescricao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ObservacoesDescricao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ObservacoesData)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ObservacoesData)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Usuario.UsuarioNome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Usuario.UsuarioNome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Objeto.ObjetoNome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Objeto.ObjetoNome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ObservacaoLocal)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ObservacaoLocal)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.ObservacoesId">Editar</a> |
    <a asp-action="Index">Voltar para a Lista</a>
</div>
